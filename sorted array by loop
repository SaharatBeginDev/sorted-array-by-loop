let arr = [3,2,1,6,4,4]
console.log(arr)
for(let i = 0; i < arr.length; i++){
    console.log(`i = ${arr[i]}`)
    for(let j = i+1; j < arr.length; j++) {
        console.log(`i = ${i}`)
        console.log(`j = ${j}`)
        console.log(`${arr[i]} > ${arr[j]}`)
        if(arr[i] > arr[j]) {
            let x = arr[i]
            arr[i] = arr[j]
            arr[j] = x
            console.log(arr)
            console.log("=======================")
        }
    }
}
console.log(arr)
